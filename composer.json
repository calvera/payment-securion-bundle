    "keywords": ["payment", "securion"],
    "homepage": "https://github.com/calvera/PaymentSecurionBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ruud Kamphuis",
            "email": "calvera@mphuis.com"
        }
    ],
    "require": {
        "php": "^5.3.2|^7.0",
        "symfony/framework-bundle": "~2.0|^3.0",
        "jms/payment-core-bundle": "~1.0",
        "omnipay/securion": "~2.0",
        "psr/log": "~1.0"
    },
	"autoload": {
        "psr-4": { "Calvera\\Payment\\SecurionBundle\\": "src" }
    }
}
